{ 
	"manifest_version": 3,
	"name": "Railgun",
	"version": "0.0.1", 
	"description": "A single-tap raiding keybinder to rival the likes of Koru",
	"icons": { 
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"content_scripts": [{
		"matches": [
			"https://*.nationstates.net/*"
		],
		"js": [ 
			"src/railgun.js"
		]
	}],
	"options_ui":{
		"page":"src/settings.html"
	},

	"permissions":["storage"]
}
